{
  "version": 2,
  
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ],

  "builds": [
      {
          "src": "server.js",
          "use": "@vercel/node",
          "config": {
              "includeFiles": [
                  "dist/**"
              ]
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "server.js"
      }
  ]
  
}